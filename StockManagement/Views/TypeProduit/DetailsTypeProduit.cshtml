@model StockManagement.Services.Refit.Contracts.Responses.TypeProduit

@section css {    
}

@{
    ViewData["Title"] = "Details Type Produit";
}

<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12 col-lg-12 col-xs-12 col-sm-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="card-title p-1">
                                <h3 class="text-center title-2">Details d'un type de produit</h3>
                            </div>
                            <hr/>
                            <div class="mt-4">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label asp-for="Id" class="control-label mb-1">
                                                Id
                                            </label>
                                            <input asp-for="Id" readonly class="form-control valid" />
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-sm-12 col-lg-6 col-xs-12">
                                        <div class="form-group">
                                            <label asp-for="Label" class="control-label mb-1">
                                                Label
                                            </label>
                                            <input asp-for="Label" readonly class="form-control valid" />
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-sm-12 col-lg-6 col-xs-12">
                                        <div class="form-group">
                                            <label asp-for="Description" class="control-label mb-1">
                                                Description
                                            </label>
                                            <textarea class="form-control" rows="4" asp-for="Description" readonly></textarea>
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-sm-12 col-lg-6 col-xs-12">
                                        <div class="form-group">
                                            <label asp-for="CreatedAt" class="control-label mb-1">
                                                Crée le
                                            </label>
                                            <textarea class="form-control" rows="4" asp-for="CreatedAt" readonly></textarea>
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-sm-12 col-lg-6 col-xs-12">
                                        <div class="form-group">
                                            <label asp-for="UpdatedAt" class="control-label mb-1">
                                                Mis à Jour le
                                            </label>
                                            <textarea class="form-control" rows="4" asp-for="UpdatedAt" readonly></textarea>
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-sm-12 col-lg-6 col-xs-12">
                                        <div class="form-group">
                                            <label asp-for="CreatedBy" class="control-label mb-1">
                                                Crée par
                                            </label>
                                            <textarea class="form-control" rows="4" asp-for="CreatedBy" readonly></textarea>
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-sm-12 col-lg-6 col-xs-12">
                                        <div class="form-group">
                                            <label asp-for="UpdatedBy" class="control-label mb-1">
                                                Mis à Jour par
                                            </label>
                                            <textarea class="form-control" rows="4" asp-for="UpdatedBy" readonly></textarea>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts {    
}